			],
			"settings": {
				"foreground": "#138808"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff6600"
			}
		},
